{"ast":null,"code":"import { getArticles } from '@/services/article';\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      menuItems: []\n    };\n  },\n  async mounted() {\n    try {\n      this.menuItems = await getArticles();\n    } catch (error) {\n      console.error('Erreur lors de la récupération des articles:', error);\n    }\n  }\n};","map":{"version":3,"names":["getArticles","name","data","menuItems","mounted","error","console"],"sources":["C:\\Users\\kubia\\KittyDelivery\\KittyDelivery_Restaurant\\frontend\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"header\">\r\n      <h1>Kitty Delivery</h1>\r\n    </div>\r\n    <div class=\"main\">\r\n      <div class=\"menu-card\">\r\n        <h2>Ma carte</h2>\r\n        <div class=\"card-items\">\r\n          <div class=\"card-item\" v-for=\"(item, index) in menuItems\" :key=\"index\">\r\n            <p>Prix : {{ item.price }} €</p>\r\n            <p>{{ item.description }}</p>\r\n            <div class=\"buttons\">\r\n              <button @click=\"modifierItem(item)\">Modifier</button>\r\n              <button @click=\"supprimerItem(item)\">Supprimer</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <button class=\"add-button\">+</button>\r\n      </div>\r\n      <div class=\"sidebar\">\r\n        <button>Mon profil</button>\r\n        <button>Mes commandes et statistiques</button>\r\n        <div class=\"delivery-status\">\r\n          <h3>Suivi de livraisons</h3>\r\n          <p>Pas de livraison en cours</p>\r\n          <div class=\"progress-bar\">\r\n            <div class=\"progress\"></div>\r\n          </div>\r\n        </div>\r\n        <div class=\"referral\">\r\n          <h3>Parrainage</h3>\r\n          <input type=\"email\" placeholder=\"Adresse mail à parrainer\" />\r\n          <button>Parrainer</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getArticles } from '@/services/article'; \r\n\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    return {\r\n      menuItems: []\r\n    };\r\n  },\r\n  async mounted() {\r\n    try {\r\n      this.menuItems = await getArticles();\r\n    } catch (error) {\r\n      console.error('Erreur lors de la récupération des articles:', error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.header {\r\n  background-color: #00ADEF;\r\n  width: 100%;\r\n  text-align: center;\r\n  padding: 1rem;\r\n  color: white;\r\n  font-size: 2rem;\r\n}\r\n\r\n.main {\r\n  display: flex;\r\n  width: 80%;\r\n  margin-top: 2rem;\r\n}\r\n\r\n.menu-card {\r\n  flex: 2;\r\n  background-color: #E0E0E0;\r\n  padding: 1rem;\r\n  margin-right: 1rem;\r\n}\r\n\r\n.card-items {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  gap: 1rem;\r\n}\r\n\r\n.card-item {\r\n  background-color: white;\r\n  padding: 1rem;\r\n  text-align: center;\r\n}\r\n\r\n.item-placeholder {\r\n  height: 100px;\r\n  background-color: #f0f0f0;\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.buttons {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.add-button {\r\n  margin-top: 1rem;\r\n  width: 100%;\r\n  padding: 1rem;\r\n  font-size: 1.5rem;\r\n  background-color: white;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.sidebar {\r\n  flex: 1;\r\n  background-color: #E0E0E0;\r\n  padding: 1rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.sidebar button {\r\n  margin-bottom: 1rem;\r\n  padding: 1rem;\r\n  background-color: white;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n}\r\n\r\n.delivery-status, .referral {\r\n  margin-top: 1rem;\r\n  padding: 1rem;\r\n  background-color: white;\r\n}\r\n\r\n.progress-bar {\r\n  width: 100%;\r\n  background-color: #f0f0f0;\r\n  height: 1rem;\r\n  border-radius: 0.5rem;\r\n  overflow: hidden;\r\n  margin-top: 0.5rem;\r\n}\r\n\r\n.progress {\r\n  width: 0%;\r\n  height: 100%;\r\n  background-color: #00ADEF;\r\n}\r\n\r\ninput[type=\"email\"] {\r\n  width: 100%;\r\n  padding: 0.5rem;\r\n  margin-bottom: 0.5rem;\r\n}\r\n</style>\r\n\r\n\r\n\r\n"],"mappings":"AAyCA,SAASA,WAAU,QAAS,oBAAoB;AAEhD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,IAAI,CAACD,SAAQ,GAAI,MAAMH,WAAW,CAAC,CAAC;IACtC,EAAE,OAAOK,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,8CAA8C,EAAEA,KAAK,CAAC;IACtE;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}